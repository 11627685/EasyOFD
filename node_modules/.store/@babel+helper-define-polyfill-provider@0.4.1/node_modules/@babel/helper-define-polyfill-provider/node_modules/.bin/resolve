#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/project/EasyOFD/node_modules/.store/resolve@1.22.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/project/EasyOFD/node_modules/.store/resolve@1.22.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../resolve@1.22.2/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../../resolve@1.22.2/node_modules/resolve/bin/resolve" "$@"
fi
