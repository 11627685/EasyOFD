#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/project/EasyOFD/node_modules/.store/rollup@3.26.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/project/EasyOFD/node_modules/.store/rollup@3.26.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/rollup@3.26.2/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../.store/rollup@3.26.2/node_modules/rollup/dist/bin/rollup" "$@"
fi
